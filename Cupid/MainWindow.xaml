<Window x:Class="Cupid.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Cupid"
        mc:Ignorable="d"
        Title="" Height="350" Width="525" WindowStartupLocation="CenterScreen" WindowState="Maximized" Icon="cupid.png">
    <Grid x:Name="grdWindow" Background="Black" MouseMove="HandleMouseMove" MouseUp="HandleMouseUp">
        <Grid x:Name="imageWrapper" MouseMove="HandleMouseMove" MouseUp="HandleMouseUp">
            <Image x:Name="imageDisplay" MouseWheel="HandleMouseWheel" MouseMove="HandleMouseMove" MouseDown="HandleMouseDown" MouseUp="HandleMouseUp" Stretch="Uniform">

            </Image>
        </Grid>
    </Grid>
</Window>
